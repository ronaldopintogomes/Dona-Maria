<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<BorderPane id="BorderPane" prefHeight="496.0" prefWidth="600.0" styleClass="background" stylesheets="@../recurso/css/padrao.css" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.donamaria.controle.MenuAdministradorControle">
   <top>
      <VBox alignment="BOTTOM_CENTER" prefHeight="101.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Label styleClass="labelTitulo" text="Menu Administrador" />
            <Label fx:id="lbFuncionarioSessao" styleClass="labelFuncionarioSessao" />
         </children>
      </VBox>
   </top>
   <left>
      <Pane prefHeight="395.0" prefWidth="345.0" BorderPane.alignment="CENTER">
         <children>
            <VBox layoutX="50.0" layoutY="30.0" prefHeight="300.0" prefWidth="250.0" spacing="10.0">
               <children>
                  <Button fx:id="btFuncionario" mnemonicParsing="false" onAction="#menuFuncionario" prefHeight="80.0" prefWidth="250.0" styleClass="textoDosBotoes" text="Funcionario" />
                  <Button fx:id="btVoltar" mnemonicParsing="false" onAction="#voltar" prefHeight="80.0" prefWidth="250.0" styleClass="textoDosBotoes" text="Voltar" />
                  <Separator prefWidth="200.0" />
                  <Button fx:id="btLogon" mnemonicParsing="false" onAction="#logon" prefHeight="80.0" prefWidth="250.0" styleClass="textoDosBotoes" text="Logon" />
               </children>
            </VBox>
         </children>
      </Pane>
   </left>
</BorderPane>
